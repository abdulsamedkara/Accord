{
  "name": "accord",
  "version": "1.0.4",
  "private": true,
  "main": "dist-electron/main.js",
  "author": "Accord Team",
  "description": "A Discord clone for your community",
  "scripts": {
    "dev": "npx tsx server/index.ts",
    "dev:next": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production npx tsx server/index.ts",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "postinstall": "prisma generate",
    "build:electron": "tsc -p electron",
    "electron:dev": "npm run build:electron && concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on tcp:3000 && electron .\"",
    "electron:build": "npm run build:electron && electron-builder",
    "package:win": "npm run build:electron && electron-packager . Accord --platform=win32 --arch=x64 --out=dist-packager --icon=resources/icon.ico --overwrite"
  },
  "build": {
    "appId": "com.accord.app",
    "productName": "Accord",
    "files": [
      "dist-electron/**/*",
      "public/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "resources/icon.ico",
      "forceCodeSigning": false
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://accord-production-fa47.up.railway.app/updates"
      }
    ],
    "linux": {
      "target": "AppImage"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.19",
    "@livekit/components-styles": "^1.2.0",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^6.1.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.7.3",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "livekit-client": "^2.17.1",
    "livekit-server-sdk": "^2.15.0",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.1.6",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "redis": "^5.10.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.3.0",
    "electron-builder": "^26.7.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^17.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}